        </key>
        <key name="ContextMenu5" file="Wincmd.ini" section="Lister" default="Search with Baidu|http://www.baidu.com/s?wd=%s" advanced="Yes" version="8.50">
          <description>Siehe Beschreibung von ContextMenu1.</description>
